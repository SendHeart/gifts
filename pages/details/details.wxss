.card-register-note {
  width: 100%;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.card-register-note input {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  margin: 0 0 20rpx 10rpx;
  padding: 15rpx;
  font-size: 24rpx;
  color: #999;
  background: #F3F3F3;
  border-radius: 14rpx;
}
.card-register-note textarea {
  width: 100%;
  height: 60rpx;
  margin: 0 0 20rpx 10rpx;
