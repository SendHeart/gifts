  width: 100%;
  margin: 0 auto;
  position: relative;
}
.picker{
  box-sizing: border-box;
  width: 100%;
  height: 50rpx;
  border:1px solid #efefef;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  font-size: 25rpx;
}
.picker .text{
  font-size: 28rpx;
  flex: 1;
}
.picker .img{
  width: 40rpx;
  height: 40rpx;
  display: block;
  transition:transform 0.3s;
}

