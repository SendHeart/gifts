@import "../../lib/style/font.wxss";
.Hui-iconfont {
  font-family: "Hui-iconfont";
  font-size:30rpx;
 }
 .iconv-uparrow:after {
   content:'\e6d6';
 }
 
 .iconv-downarrow:after {
   content:'\e6d5';
 }

page {
	background: #f2f2f2;
  width: 100%;
  height: 100%;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.startBar {
  height: 75rpx;
  background: #333;
  width: 100%;
  position: fixed;
  top: 0;
  padding: 5rpx;
  border-bottom: 0px solid #f2f2f2;
  z-index: 99;
}
.top-title {
  height: 75rpx;
  background: #333;
  width: 100%;
  position: fixed;
  top: 0;
  padding: 5rpx;
  border-bottom: 0px solid #f2f2f2;
  z-index: 99;
}
.top-title-item {
  display: flex;
  flex-direction: row;
  width: 100%;
  color: #fff;
  text-align: center;
  line-height: 75rpx;
  font-size: 48rpx;
  padding: 10rpx;
}
.top-title-item .title-image {
  width: 70rpx;
  height: 70rpx;
  margin-right: 1rpx;
  margin-top: 0rpx;
  margin-left: 20rpx;
}
.banner {
	width: 100%;
  height: 100rpx;
	/*margin-left: 15rpx;*/
  background-color:white;
  background-repeat:repeat;
  padding-top: 35rpx;
  opacity:1;
  /*
  position: fixed;
  top:0;
  z-index:99;
  */
}

.banner image {
	width: 100%;
	height: 200rpx;
}

.banner .slogan{  
  border: solid 0px black;  
  display: flex;  
  flex-direction: row;  
  justify-content: center;  
  margin:5px;
}  
.memcard {
  display:flex;
  flex-direction: column;
  justify-content: left;
  align-items:left;
  width: 96%;
  border-radius: 20rpx;
  margin:10rpx auto;
  padding-bottom: 30rpx;
  background: linear-gradient(30deg,#222 25%,#1d1d1d 0,#1d1d1d 50%,#222 0,#222 75%,#1d1d1d 0);
  background-size: 500px 1200px;
}

/*user info*/
.userinfo {
  display: flex;
  flex-direction:row;
  justify-content:center;
  margin: 20rpx auto;
  height: 90rpx;
}
.userinfo-avatar {
  width: 90rpx;
  height: 90rpx;
  border-radius: 40%;
}

.userinfo-nickname {
  color: #FFCA4B;
  font-size: 30rpx;
  font-weight: bold;
  margin: 0 20rpx;
  line-height: 90rpx;
  display: flex;
  flex-direction: column;
  justify-items: center;
  align-items: center;
}
.userinfo-cards-info{
  display:flex;
  flex-direction: row;
  justify-content: flex-start;
  height: 100rpx;
}
.userinfo-cards-logo {
  width: 80rpx;
  height: 80rpx;
  margin: 20rpx 10rpx 0 20rpx;
  border-radius: 40%;
}
.userinfo-cards-name{
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 10rpx;
  width: 45%;
}
.userinfo-cards-title {
  font-size: 28rpx;
  color: #FFCA4B;
  line-height: 26rpx;
  font-weight: bold;
}
.userinfo-cards-mid{
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 10rpx;
  margin-right: 20rpx;
  width: 30%;
}
.cards-id{
  color: #FFCA4B;
  font-size: 22rpx;
  font-weight: normal;
  line-height: 22rpx;
}
.scanqr {
  width: 50rpx;
  height: 50rpx;
  margin-top: 0rpx;
  margin-left: 10rpx;
  border-radius: 50%;
}

.userinfo-cards-item{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin:10rpx auto;
  width: 100%;
}

.userinfo-cards-nickname{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin-left: 0rpx;
  width: 100%;
}
.userinfo-cards-no {
  color: #FFCA4B;
  font-size: 30rpx;
  font-weight: normal;
  margin-right: 20rpx;
}

.userinfo-cards-due {
  color: rgb(235, 235, 235);
  font-size: 22rpx;
  font-weight: normal;
  margin-right: 20rpx;
}
.userinfo-cards-expend {
  color: rgb(235, 235, 235);
  font-size: 22rpx;
  font-weight: normal;
}

.cardrenew {
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.cardrenew-btn {
  height: 60rpx;
  width: 80%;
  background: #FF952D;
  color:#fff;
  font-size: 26rpx;
  margin: 0 auto;
  text-align: center;
  line-height: 60rpx;
  border-radius: 10rpx;
}

/*order*/
.menu-area {
  /*
  display: flex;
  flex-direction: column;
  justify-content: center;
  */
  width: 96%;
  background: #fff;
  color:#444;
  border-radius: 10rpx;
  margin: 0 auto;
}

/*menu list*/
.tableviewcell {
	width:100%;
  height: 100%;
  padding: 20rpx;
	color: #fff;
	display: flex;
  flex-direction: row;
	align-items: center;
}
.linegray {
	border-bottom: 1rpx solid #333;
}
/*icon*/
.tableviewcell image {
	width: 40rpx;
	height: 40rpx;
}

/*text*/
.tableviewcell text {
	font-size: 28rpx;
	margin-left: 20rpx;
}
.inlinetips {
  color: #999;
  font-size: 28rpx;
}

/*button*/
.logout {
	margin: 30rpx;
  border: 0;
  background: transparent;
  color: #E34C55;
  font-size: 24rpx;
}
.logout::after {
  border: 0;
}

/*order*/
.order {
  display:inline-block;
  width: 20%;
  margin-top: 10rpx;
  text-align: center;
}

.order image {
  padding: 10rpx 10rpx 0 10rpx;
  width: 60rpx;
  height: 60rpx;
  border-radius: 40%;
  /* background: #F2F2F2;
  border-radius: 40%; */
}

.order text{
	display: block;
	font-size: 24rpx;
  color: #333;
  line-height: 40rpx;
  height: 40rpx;
}

/** 修改button默认的点击态样式类**/
.button-hover {
  background-color: red;
}
/** 添加自定义button点击态样式类**/
.other-button-hover {
  background-color: blue;
}

.order .clickBtn{
  width: 150rpx;  
  line-height: 40rpx;
  height: 40rpx;
  display: block;
  font-size:24rpx;
  background-color:#fff;  
  color:#444;  
  overflow:hidden;  
  border: 0;
}  

.clickBtn::after {
  border: 0;
} 


.shopping_container {
  display: flex;
  flex-direction: column;
  justify-content: center; /*子项横轴上居中对齐*/
  text-align: center;
  background: #fff;
  border-top: 1rpx solid #eee;
  padding: 10rpx 0 40rpx 0;
  margin-bottom: 10rpx;
  z-index: 1;
}

.shopping_container .img_tag {
  width: 10%;
  height: 30rpx;
  margin: 10px;
}

.shopping_container .goods-name {
  display: flex;
  flex-direction: column;
  align-content: center;
  width: 100%;
  font-size: 36rpx;
  font-weight: bold;
  color: #444;
}

.shopping_container .goods_prom {
  display: block;
  color: #999;
  font-size: 28rpx;
  font-weight: normal;
  white-space: nowrap;
  overflow-x: hidden;
}

.shopping_container .goods-info {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0;
  font-size: 32rpx;
}
prd-detail {
  margin: 0;
  padding: 0;
  color: #999;
}
.prd-detail text {
  font-size: 28rpx;
}
.modalbox {
  background: #ffffff;
}
modal {
  width: 100% !important;
}
.wxParse {
  line-height: 1.8;
  font-size: 24rpx;
  height: 600rpx;
  }
 
.picker_box{
  background: #fff;
  width: 100%;
  margin: 5rpx auto;
  position: relative;
}

.picker{
  box-sizing: border-box;
  width: 100%;
  height: 70rpx;
  border:1px solid #efefef;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  font-size: 35rpx;
}
.picker .text{
   text-align: center;
  font-size: 35rpx;
  flex: 1;
}
.picker .img{
  width: 40rpx;
  height: 40rpx;
  display: block;
  transition:transform 0.3s;
}


.sentbtn {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-content: center;
  margin: 20rpx auto;
  width: 100%;
  height: 160rpx;
}

.sentbtn button {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  background: #1d1d1d;
  color: #E34C55;
}

.sentbtn text {
  font-size: 24rpx;
  color: #999;
}
.modalbox-text {
  font-size: 28rpx;
  color: #666;
  text-align: center;

}
.sentbtn input{
  width: 400rpx;
  height:60rpx;
  line-height: 60rpx;
  border: 2rpx solid #eee;
  margin: 20rpx auto;
  border-radius:10rpx; 
  padding: 4rpx 20rpx;
  background: #f2f2f2;
}

.radiog {
  text-align: center;
}
radio .wx-radio-input{
  height: 26rpx;
  width: 26rpx;
  /* 自定义样式.... */
} 
radio .wx-radio-input.wx-radio-input-checked::before{
  background-color: #e02e24;
  border: 1rpx solid #e02e24;
  /* 自定义样式.... */
}

.modal-bg {
  height: 100%;
  width: 100%;
  position: fixed;
  background-color:rgba(0, 0, 0, .5);
  z-index: 2;
  top: 0;
  left: 0;
}

.modal-content-bg {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:center;
  background-color:#fff;
  margin: 45% 10%;
  padding: 30px 10px;
  border-radius: 20rpx;
  border: 3rpx solid rgb(189, 236, 248);
}

.modal-text {
  text-align: center;
  font-size: 32rpx;
  color: #333333; 
}

.modal-btn {
  text-align: center;
  font-size: 28rpx;
  padding: 0rpx 30rpx;
  color: #f2f2f2; 
  border-radius: 20rpx;
  background-color:#E34C55;
  border: 3rpx solid #E34C55;
  margin-top: 40rpx;
}

/*推荐商品列表头*/
.recomment-title {
  display: flex;
  flex-direction: row;
  justify-content: space-between; /*子项横轴上对齐*/
  padding:10rpx 4%;
  margin-top: 20rpx;
  font-weight: bold;
}
.recomment-title text{
  width: 350rpx;
  color: #444;
  font-size: 22rpx;
}
.recomment-title text.title_ex{
  color: #ccc;
  font-size: 24rpx;
  font-weight: normal;
  margin-left: 10rpx;
}
.recomment-title text.more {
  font-size: 24rpx;
  line-height: 50rpx;
  color: #ccc;
  font-weight: normal;
  text-align: right;
}

.recomment-title button {
  line-height: 60rpx;
  text-align: center;
  width: 150rpx;
  height: 60rpx;
  background-color: #E52445;
  color: white;
  font-size: 30rpx;
  margin:0 auto;
  border-radius: 10rpx;
  padding:0 auto;
}


.venues-box {
  width: 96%;
  margin:0 auto;
  padding-bottom: 60rpx;
  overflow: hidden;
  z-index: 1;
}

.venues-item-main {
  margin-bottom:10rpx;
  border-radius: 10rpx;
  background: #fff;
  box-sizing: border-box;
   /* display: inline-block; */
  display: flex;
  flex-direction: row;
  height:275rpx;
  position: relative;
  color: #444444;
}
.venues-item-category {
  margin-bottom:15rpx;
  border-radius: 14rpx;
  background: #fff;
  box-sizing: border-box;
  display: inline-block;
  color: #444444;
}


/*商品信息描述*/
.venues-item-text {
	margin-left: 10rpx;
	width: 65%;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
}
.box-left,.box-right{
  display: inline-block;
  vertical-align: top;
  width: 49%;
}
.box-right{
  float: right;
}

.main-mainpic {
  width: 35%;
  height: 255rpx;
  border-radius: 14rpx;
}

.category-mainpic {
  display: block;
  width: 355rpx;
  height: 355rpx;
}

.goods-name {
  color: #444444;
  font-size: 26rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  padding:10rpx auto;
  padding-left: 10rpx;
}


.goods-tags {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding:10rpx 0 0 10rpx;
}

.goods-prom {
  display: block;
  color: #999;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: wrap;
  padding:5rpx 10rpx;
}

.price-list {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding:10rpx;
}

.price-market {
  text-indent: 0;
  color: #999;
  font-size: 24rpx;
  line-height: 1.8;
  text-decoration:line-through;
  margin-right: 4rpx;
  display: none;
}
.price-market::first-letter {
  font-size: 20rpx;
}
.price-now {
  text-indent: 0;
  color: #bb7e0c;
  font-size: 26rpx;
  margin-right: 24rpx;
  line-height: 1.8;
}

.price-now::first-letter {
  font-size: 20rpx;
}

.left-tag {
  height: 28rpx;
  line-height: 28rpx;
  color: #fc9326;
  font-size: 20rpx;
  text-align: center;
  border-radius: 6rpx;
  padding: 6rpx 6rpx;
}

.img-discount {
  color: #fff;
  font-size: 22rpx;
  background: #EC4844;
  text-align: center;
  padding: 0 8rpx;
  border-radius: 6rpx;
  margin-right: 8rpx;
}

.goods-org {
  color: #fff;
  font-size: 22rpx;
  background: #FF952D;
  text-align: center;
  padding: 0 8rpx;
  border-radius: 6rpx;
  margin-right: 8rpx;
}
.goTop{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;  
  height: 80rpx;
  width: 80rpx;
  position: fixed;
  margin-bottom: 10px;
  bottom: 0;
  background: #666;
  right: 30rpx;
  border-radius: 50%;
  color:#fff;
  font-size: 8;
  z-index: 1000;
  opacity:0.5;
}