@import "../../lib/style/font.wxss";
 
.Hui-iconfont {
 font-family: "Hui-iconfont";
 font-size:24rpx;
}
.iconv-uparrow:after {
  content:'\e6d6';
}
.iconv-downarrow:after {
  content:'\e6d5';
}
page {
  background: #1d1d1d;
  overflow-x: hidden;
  width: 100%;
  height: 100%;
}
/* 隐藏滚动条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}


/* 返回顶部 */
.goTop{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;  
  height: 80rpx;
  width: 80rpx;
  position: fixed;
  bottom: 30rpx;
  background: rgba(0,0,0,.3);
  right: 30rpx;
  border-radius: 50%;
  color:#fff;
  font-size: 8;
  z-index: 1000;
}

/*外部容器*/
.container {
  width: 100%;
  overflow: scroll;
  overflow-x: hidden;
  margin: 0 auto;
  box-sizing: border-box;
  z-index: 0;
}

.search {
	width: 100%;
	height: 72rpx;
  display: flex;
	flex-direction: row;
	justify-content: space-between;/*子项横轴上居中对齐*/
	align-items: center;/*子项纵轴上居中对齐*/
  padding: 0rpx 0rpx 8rpx 0rpx;
  background: #1d1d1d;
  margin-bottom: 10rpx;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  width:10%;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  position: relative; 
  margin:7px 5px;
}

.userinfo-avatar {
  width: 60rpx;
  height: 60rpx;
  margin-right:0rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  font-size: 24rpx;
  font-weight: bold;
  color:#999;
  text-overflow: ellipsis;
  overflow-x: hidden;
  white-space: nowrap;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical; 
}
.search .wx-input {
  display: flex;
  flex-direction: row;
	width: 20%;
	height: 56rpx;
  /*background: #444;*/
	border-radius: 120rpx;
  padding-left: 10rpx;
  text-align: left;
  z-index: 99;
  
}
.search .wx-input image {
  width: 34rpx;
	height: 34rpx;
  margin: 12rpx 2rpx 12rpx 10rpx;
}
.search .wx-input icon {
  width: 34rpx;
	height: 34rpx;
  margin: 12rpx 2rpx 12rpx 10rpx;
}

.search .wx-input text {
  font-size: 24rpx;
  color: #999;
  text-indent: 10rpx;
  line-height: 60rpx;
}
.messages {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: right;
  position: relative;
  width: 100rpx;
  margin-right: 20rpx;
}
.messages .message-num {
  position: absolute;
  top: 0rpx;
  right: 12rpx;
  text-align: center;
  color: #fff;
  background: rgb(227, 76, 85);
  font-size: 18rpx;
  width:28rpx;
  height:28rpx;
  border-radius: 50%;
}

.icon-message {
  width: 40rpx;
  height: 40rpx;
}

modal {
  margin: 0;
  padding: 0;
  border: 0;
}

.startBar {
  height: 75rpx;
  background: #333;
  width: 100%;
  position: fixed;
  top: 0;
  padding: 5rpx;
  border-bottom: 0px solid #f2f2f2;
  z-index: 99;
}

.banner {
  width: 100%;
  height: 200px;
  background: linear-gradient(0deg, #f2f2f2 50%, #1d1d1d 50%);
  align-content: center;
  z-index: 9;
  padding: 16rpx 0;
}

.banner image {
  width: 100%;
  height: 200px;
  z-index: 9;
}

.banner .slogan {
  border: solid 0px black;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin: 5px;
}
.swiper-box {
  width: 96%;
  height: 100%;
  margin: 0 auto;
  /*
  left: 2%;
  top: 82rpx;
  */
  z-index: 1000
}
.slide-image {
  border-radius: 8rpx;
  display: block;
  height: auto;
}
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{
  margin-bottom: 2rpx;
}
.swiper-box .wx-swiper-dot{
 width:30rpx;
 display: inline-flex;
 height: 6rpx;
 margin-left: 8rpx;
 justify-content:space-between;
}
.swiper-box .wx-swiper-dot::before{
 content: '';
 flex-grow: 1; 
 background: rgba(255,255,255,0.35);
 border-radius: 8rpx
}
.swiper-box .wx-swiper-dot-active::before{
 background:rgba(0,0,0,0.35);   
}

