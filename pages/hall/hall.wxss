/*外部容器*/

.container {
  width: 96%;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  z-index: 1;
}

/*整体列表*/

.container .carts-list {
  display: flex;
  flex-direction: column;
  padding-left: 10rpx;
}

/*item与button的容器*/

.carts-container {
  position: relative;
  width: 100%;
  min-height: 160rpx;
  z-index:1;
}

/*每行单元格*/

.carts-item {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  /*width属性解决标题文字太短而缩略图偏移*/
  padding: 20rpx 0;
  margin-right: 20rpx;
  position: absolute;
  top: 0;
  left: 0;
  background: #fff;
  z-index: 1;
}

/*左部图片*/

.carts-image {
  width: 120rpx;
  height: 120rpx;
  margin: 0 20rpx;
}

/*右部描述*/

.carts-text {
  width: 75%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

/*右上部分标题*/

.carts-title {
  font-size: 24rpx;
  color: #444;
  line-height: 1.4;
  padding-right: 10rpx;
  text-overflow: ellipsis;
  overflow-x: hidden;
  white-space: nowrap;
}

.carts-sku {
  font-size: 24rpx;
  color: #999;
  line-height: 1;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

/*右下部分价格与数量*/

.carts-subtitle {
  font-size: 24rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

/*价格*/

.carts-gov-price {
  color: #999;
  font-size: 24rpx;
  line-height: 18px;
  width: 150rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.carts-en-price {
  color: #444;
  font-size: 24rpx;
  line-height: 2;
  width: 150rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.subtitle2 {
  background: white;
  width: 100%;
  height: 6rpx;
  border-radius: 0 0 10rpx 10rpx;
}

/*人民币符号*/

.carts-price:before, .select-and-amount text:last-child:before {
  content: '';
}

/*商品加减按钮*/

/*stepper容器*/

.stepper {
  width: 210rpx;
  height: 48rpx;
  margin: 0 auto;
  position: absolute;
  right: 28rpx;
  bottom: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

/*加号与减号*/

.stepper text {
  width: 48rpx;
  height: 40rpx;
  line-height: 38rpx;
  text-align: center;
  border: 2rpx solid #ccc;
  border-radius: 14rpx;
  font-size: 32rpx;
}

/*数值*/

.stepper input {
  color: #444;
  float: left;
  margin: 0;
  width: 40rpx;
  height: 40rpx;
  text-align: center;
  font-size: 24rpx;
}


/*普通样式*/

.stepper .normal {
  color: #ddd;
}

/*禁用样式*/

.stepper .disabled {
  color: #ccc;
}

/*复选框样式*/

.carts-list icon {
