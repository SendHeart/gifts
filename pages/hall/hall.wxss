page {
  background: #F3F3F3;
  overflow-x: hidden;
  width: 100%;
  height: 100%;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}
modal {
  margin: 0;
  padding: 0;
  border: 0;
}
.startBar {
