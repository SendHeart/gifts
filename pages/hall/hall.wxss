/*整体列表*/

.carts-list {
  display: flex;
  flex-direction: column;
  margin: 0 auto;
  width: 96%;
}

/*item与button的容器*/

.carts-container {
  width: 100%;
  height: 150rpx;
  padding: 20rpx 0 0 0;
  background: #444;
  z-index:1;
}

/*每行单元格*/

.carts-item {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  /*width属性解决标题文字太短而缩略图偏移*/
  z-index: 1;
}

/*左部图片*/

.carts-image {
  width: 90rpx;
  height: 90rpx;
  margin: 0 20rpx;
}

/*右部描述*/

.carts-text {
  width: 75%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

/*右上部分标题*/

.carts-title {
  font-size: 24rpx;
  color: #eee;
  line-height: 150%;
  padding-right: 10rpx;
  text-overflow: ellipsis;
  overflow-x: hidden;
  white-space: nowrap;
}

.carts-sku {
  font-size: 24rpx;
  color: #999;
  line-height: normal;
}

/*右下部分价格与数量*/

.carts-subtitle {
  font-size: 24rpx;
}

/*价格*/

.carts-gov-price {
  color: #999;
  font-size: 24rpx;
  line-height: 18px;
  width: 150rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.carts-en-price {
  color: #eee;
  font-size: 24rpx;
  line-height: 2;
  width: 150rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.subtitle2 {
  background: #444;
  width: 100%;
  height: 6rpx;
  border-radius: 0 0 10rpx 10rpx;
}

/*人民币符号*/

.carts-price:before, .select-and-amount text:last-child:before {
  content: '';
}

/*商品加减按钮*/

/*stepper容器*/

.stepper {
  width: 210rpx;
  height: 48rpx;
  margin: 0 auto;
  position: absolute;
  right: 28rpx;
  bottom: 10rpx;
  display: none;
  flex-direction: row;
  justify-content: flex-start;
}

/*加号与减号*/

.stepper text {
  width: 40rpx;
  height: 40rpx;
  line-height: 38rpx;
  text-align: center;
  border: 2rpx solid #ccc;
  border-radius: 50%;
  font-size: 32rpx;
}

/*数值*/

.stepper input {
  color: #444;
  float: left;
  margin: 0;
  width: 40rpx;
  height: 40rpx;
  text-align: center;
  font-size: 24rpx;
}


/*普通样式*/

.stepper .normal {
  color: #999;
}

/*禁用样式*/

.stepper .disabled {
  color: #ccc;
}

/*复选框样式*/

.carts-list icon {
  margin-top: 60rpx;
  margin-right: 20rpx;
}

/*商品列表头*/

