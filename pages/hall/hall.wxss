	.uni-tip {
		padding: 15px;
		width: 100%;
		background: #fff;
		box-sizing: border-box;
		border-radius: 10px;
		margin-bottom: 0rpx;
	}

	.uni-tip-title {
		text-align: center;
		font-weight: bold;
		font-size: 16px;
		color: #333;
	}

	.uni-tip-content {
		padding: 15px;
		font-size: 14px;
		color: #666;
	}

	.uni-tip-group-button {
		margin-top: 10px;
		display: flex;
	}

	.uni-tip-button {
		width: 100%;
		text-align: center;
		font-size: 14px;
		color: #3b4144;
	}
	.placeholder {
		opacity: 1;
		transition: opacity 0.4s linear;
	}
	
	.placeholder.loaded {
		opacity: 0;
	}
	
