.goods-name {
  color: #111111;
  font-size: 28rpx;
  padding-top:5rpx;
}

.goods-name2 {
  color: #fff;
  font-size: 26rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  padding:10rpx auto;
  padding-left: 10rpx;
}

.goods-tags {
  color: #fff;
  font-size: 22rpx;
  background: #ff952d;
  text-align: center;
  padding: 4rpx 8rpx;
  border-radius: 6rpx;
  margin-right: 8rpx;
}
.goods-tags2 {
  padding: 4rpx 10rpx;
}
.goods-titletext {
  flex-wrap:wrap;
  display:flex;
  flex-direction:row;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  padding: 0 10rpx;
}

.goods-prom {
  display: block;
  color: #999;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: wrap;
  padding:5rpx 10rpx;
}

.price-list {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding:10rpx;
}

.price-market {
  text-indent: 0;
  color: #999;
  font-size: 24rpx;
  line-height: 1.8;
  text-decoration:line-through;
  margin-right: 4rpx;
  display: none;
}
.price-market::first-letter {
  font-size: 20rpx;
}
.price-now {
  text-indent: 0;
  color: #e34c55;
  font-size: 30rpx;
  margin-right: 24rpx;
  line-height: 1.8;
  font-weight:bold;
}

.price-now .sfont {
  font-size: 20rpx;
}

.left-tag {
  height: 28rpx;
  line-height: 28rpx;
  color: #fc9326;
  font-size: 20rpx;
  text-align: center;
  border-radius: 6rpx;
  padding: 6rpx 6rpx;
}

.main-prom {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.main-message{
  position:fixed;
  width:100%;
  top:0px;
  background:rgba(0,0,0,0.4);
  overflow: hidden;
  z-index: 9999;
}
.main-message .t_w{
  position:relative;
  display: flex;
  align-items: center;
  justify-content: center;  
  width: 100%;
  height: 100%;
}
.main-message .tanchu_view{
  width: 80%;
  margin:10% auto;
  overflow: hidden;
  background-color: #fff1e6;
  border-radius: 10rpx;
  padding: 4rpx;
}

.main-message .main-red{
  display: flex;
  align-items: center;
  justify-content: center;
}
.main-message .bg_view{
  margin:0rpx auto 30rpx auto;
  color:#fcb712;
  font-size:50rpx;
  background-color: #fff1e6;
  padding:0rpx 30rpx;
  text-align: center;
}

.main-message .txtys{
    font-size: 50rpx;
    text-align: center;
    margin-bottom: 200rpx;
}
 
.main-message .txtsure{
 width: 100%;
 height: 100%;
 display: flex;
 justify-content: center;
 margin: 20rpx;
}
 
/*确定背景*/
.main-message .txtsurebg{
  width: 100rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  margin: 150rpx;
  background-color: burlywood;
  flex-direction: column;
}
 
/*确定图标*/
.main-message .txtsureimg{
  display:block;
  width:60rpx;
  height:60rpx;
}
 
/*确定文本*/
.main-message .txtsurename{
   margin-bottom: 10rpx;
   color: white;
   font-size: 30rpx;
}
 
/*右上角图标*/
.main-message .t_image{
  position: absolute;
  top: 15%;
  right:30rpx;
}
 
/*右上角图标*/
.main-message .t_image1{
  display:block;
  width:60rpx;
  height: 60rpx;
  opacity:0.8;
}

