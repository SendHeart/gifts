 }
.container{
  margin-bottom: 50rpx;
  margin-top: 110rpx;
  overflow: hidden;
}


/* 显示与隐藏 */
.show {
	display: block;
}

.hidden {
	display: none;
}

.msg-list {
 	display: flex;
	flex-direction: column;
	width:96%;
  text-align: left;
  align-items: left;
  background-color: #fff;
  border-radius: 10rpx;
  padding-bottom: 20rpx;
  margin-bottom: 15rpx;
}
.msg-title {
  margin:15rpx 20rpx;
  display: flex;
  flex-direction: row;
  font-size: 24rpx;
  color: #999;
  justify-content:space-between;
}
.msg-title text {
  display: inline-block;
  margin-right: 10rpx;
}

.msg-item {
 	display: flex;
	flex-direction: row;
	width:96%;
	padding: 0;
  text-align: left;
  align-items: left;
  background-color: #fff;
  border-top: 1rpx solid #eee;
  margin: 0 auto;
}
.msg-item-image {
	width:140rpx;
  height:140rpx;
  text-align: center;
  align-items: center;
}

.msg-item-content {
 	display: flex;
	flex-direction: column;
	width:75%;
	padding: 0;
  text-align: left;
  align-items: left;
}
 
.msg-item-text{
  margin:20rpx 20rpx 0 20rpx;
  font-size:28rpx;
  overflow:hidden;  
  text-overflow:ellipsis;  
  white-space:wrap; 
  color: #e34c55;
}
.msg-item-text2{
  margin-left:20rpx;
  margin-right:20rpx;
  font-size:28rpx;
  overflow:hidden;  
  text-overflow:ellipsis;  
  white-space:wrap; 
  color: #444;
}
.msg-item-text3{
  margin-right:20rpx;
  font-size:28rpx;
  font-weight: bold;
  color: #444;
}

.msg-content-footer {
  margin-left:20rpx;
  font-size:28rpx;
  overflow:hidden;  
  text-overflow:ellipsis;  
  white-space:wrap; 
  color: #444;
}
 
.msg-content-image image {
 /*被图片设置宽高*/ 
  width: 40px; 
  height: 40px; 
  padding-left:10px;
  padding-bottom: 10rpx;
  
}

.morebtn {
  margin:20rpx auto 200rpx auto;
  border:none;
  color: #fff;
  font-size: 24rpx;
}
