.page-body-button {
  margin-bottom: 30rpx;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}

live-player {
  width: 100%;
  height: 225px;
}
.slide-image {
  width: 100%;
  height: 950rpx;
  display: inline-block;
  overflow: hidden;
  z-index:1;
}
.goods-list {
  width:100%;
  position:fixed;
  display:flex;
  flex-direction:row;
  justify-content: space-between;
  border:0px solid #aaaaaa;
  top: 20rpx;
  padding-left:2px;
  padding-bottom:10px;
  overflow: hidden;
  opacity:1;
  z-index: 999;
}
.live-title {
  width: 50%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  height: 80rpx;
  line-height: 35rpx;
  /*width属性解决标题文字太短而缩略图偏移*/
  border-bottom: 1rpx solid #f2f2f2;
  padding: 5rpx;
  background: #333;
  opacity: 0.8;
  border-radius:50rpx;
  margin: 0 auto; 
}
.live-title image{
  width:80rpx;
  height: 80rpx;
  border-radius:50%;
  margin:0 auto auto 10rpx;
}
.live-title-text {
  width: 60%;
  display: flex;
  flex-direction: column;
  justify-content:flex-start;
}

.live-title-text text {
  font-size: 24rpx;
  height: 40rpx;
  padding: 5rpx;
}
.live-title-name {
  font-size: 25rpx;
  color: #fff;
  overflow:hidden;  
  text-overflow:ellipsis;  
  white-space:nowrap; 
}

.live-title button { 
  width:130rpx;
  height:40rpx;
  line-height : 40rpx ;
  margin:20rpx auto;
  font-size : 22rpx ;
  padding:5rpx auto;
  background: #e34c55;
  border-radius: 50rpx;
  color: #fff;
  opacity:0.4;
}
.goods-list label { 
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  width: 50%;
  height:80rpx;
  line-height: 80rpx;
  font-size:20rpx;
  opacity:0.6;
}
.live-member-image{
  width:75%;
  height: 60rpx;
  margin-top:10rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  z-index: 999;
  opacity:1;
}
.live-member-image image{
  width: 60rpx;
  height:60rpx;
  margin:5rpx auto;
  position: absolute;
  border-radius:50%;
  z-index: 999;
  opacity:0.8;
}
.live-on{
  background: #999;
  color:#fff;
  border-radius: 50rpx;
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding: 5rpx;
  text-align: center;
  margin:20rpx 20rpx auto;
}

.footer{
  width: 100%;
  position:fixed;
  display:flex;
  flex-direction:row;
  justify-content: flex-start;
  bottom: 0rpx;
  overflow: hidden;
  opacity:1;
  z-index: 999;
}
.footer-left {
  width:100%;
  position:relative;
  display:flex;
  flex-direction:row;
  justify-content: flex-start;
  border:0px solid #aaaaaa;
  margin-left:20rpx;
}
.footer-button{
		height: 120rpx;
		width: 120rpx;
		color: #fff;
		position: relative;
		text-align: center;
}

.goods-num {
  width: 60rpx;
	height:60rpx;
	position: absolute;
	text-align: center;
	top:20rpx;
	left:0rpx;
  padding: 15rpx;
  font-size:22rpx;
  color:#fff;
}

.goods-button{
		/* border: 6upx solid #F44336; */
		width: 60rpx;
		height: 60rpx;
		background: rgb(235, 74, 63);
		border-radius: 50%;
		position: absolute;
		text-align: center;
		top:0;
		left: 0rpx;
    padding: 15rpx;
		box-shadow: 0 0 28upx 0 rgb(251, 99, 24);
    opacity: 0.8;
}
