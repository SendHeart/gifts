.startBar {
  background: rgba(10, 10, 10, 0.9);
  width: 100%;
  position: fixed;
  top: 0;
  z-index: 99;
}
.top-title {
  height: 88rpx;
  background: rgba(10, 10, 10, 0.9);
  width: 100%;
  position: fixed;
  top: 0;
  z-index: 99;
}
.top-title-item {
  display: flex;
  flex-direction: row;
  justify-content:flex-start;
  align-items: center;
  text-align: center;
  width: 100%;
  color: #fff;
  line-height: 88rpx;
  font-size: 40rpx;
  font-weight: bold;
}
.top-title-item .title-image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 1rpx;
  margin-top: 0rpx;
  margin-left: 30rpx;
}
.top-title-item .title-text { width: 100%; margin-left: -78rpx;}
.amount:before {
	content: '￥';	
}

.submit {
	margin: 8px;
}

/*单元格*/
.cell {
	display: flex;
	flex-direction: row;
	justify-content: flex-start;
	align-items:center;
	margin: 4px;
	border-bottom: 1px solid #eee;
	line-height: 20px;
  width:100%;
}

.cell:last-child {
	border-bottom: 0;
}

/*label显示字符*/
.cell text:first-child {
	width: 80px;
}

/*请先填写地址*/
.area_select {
	width: 100%;
}

/*选择器*/
picker {
	width: 60%;
}