page {
	background: #F2F2F2;
  overflow-x: hidden;
  width: 100%;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.startBar {
  height: 75rpx;
  background: #333;
  width: 100%;
  position: fixed;
  top: 0;
  padding: 5rpx;
  border-bottom: 0px solid #f2f2f2;
  z-index: 99;
}
.top-title {
  height: 75rpx;
  background: #333;
  width: 100%;
  position: fixed;
  top: 0;
  padding: 5rpx;
  border-bottom: 0px solid #f2f2f2;
  z-index: 99;
}
.top-title-item {
  display: flex;
  flex-direction: row;
  width: 100%;
  color: #fff;
  text-align: center;
  line-height: 75rpx;
  font-size: 48rpx;
  padding: 10rpx;
}
.top-title-item .title-image {
  width: 70rpx;
  height: 70rpx;
  margin-right: 1rpx;
  margin-top: 0rpx;
  margin-left: 20rpx;
}
.amount {
color:red;
font-size:24rpx;
}
.amount:before {
	content: '￥';	
}

.submit {
	margin: 8px;
}

/*单元格*/
.cell {
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items:left;
	border-top: 1rpx solid #eee;
	line-height: 1.5;
  width:100%;
  background: #fff;
  font-size: 26rpx;
  padding: 15rpx 30rpx;
}
.amount_item {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  line-height: 2;
  width: 90%;
}
.cell:last-child {
	display: flex;
  flex-direction: row;
  align-items: center;
  background: #fff;
  font-size: 28rpx;
  padding: 15rpx 30rpx;
}

/*label显示字符*/
.cell text:first-child {
	width: 80px;
}

.carts-more {
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	border-top: 1rpx solid #eee;
	border-bottom: 1rpx solid #eee;
	line-height: 1.5;
  width:100%;
  background: #fff;
  font-size: 24rpx;
  padding: 15rpx 30rpx;
}

/*请先填写地址*/
.area_select {
	width: 100%;
}

/*选择器*/
picker {
	width: 60%;
}


.gifts-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  align-content: center;
  text-align: center;
  /*width属性解决标题文字太短而缩略图偏移*/
  width: 100%;
  margin-bottom: 10rpx;
}
.gifts-image {
  margin: 10rpx;
  width: 450rpx;
  height: 270rpx;
}
.morebtn {
  margin: 20rpx auto;
  background: #fff;
  color: #e34c55;
  font-size: 28rpx;
  width: 70%;
}

/*外部容器*/

.container {
  width: 100%;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  z-index: 1;
}

/*整体列表*/

.container .carts-list {
  display: flex;
  flex-direction: column;
}

/*item与button的容器*/

.carts-container {
  position: relative;
  width: 100%;
  height: 260rpx;
}

/*每行单元格*/

.carts-item {
  width: 100%;
  display: flex;
  flex-direction: row;
  height: 200rpx;
  /*width属性解决标题文字太短而缩略图偏移*/
  border-bottom: 1px solid #F2F2F2;
  padding: 20rpx 0;
  margin-right: 20rpx;
  position: absolute;
  top: 0;
  left: 0;
  background: #fff;
  z-index: 1;
}

/*左部图片*/

.carts-image {
  width: 120rpx;
  height: 120rpx; 
  margin: 0 20rpx;
}

/*右部描述*/

.carts-text {
  width: 70%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

/*右上部分标题*/

.carts-title {
  font-size: 26rpx;
  color: #444;
  line-height: 32rpx;
}

/*右下部分价格与数量*/

.carts-subtitle {
	display: flex;
	flex-direction: row;
	justify-content:space-between;
  height: 40rpx;
}
.carts-sku {
font-size:26rpx;
color:#444;
 
}
.carts-sku text{
height:40rpx;
line-height: 40rpx;
}

/*价格*/

.carts-price {
  height:30rpx;
  line-height: 30rpx;
  font-size: 26rpx;
}

.carts-gov-price {
  color: #999;
  font-size: 24rpx;
  line-height: 1;
  width: 150rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.carts-en-price {
  color: #FF952D;
  font-size: 12px;
  line-height: 18px;
  width: 150rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.order-item {
  width: 100%;
  position: relative;
  /*
  top: 301rpx;
  left: 0;
  */
  background: #fff;
  z-index: 1;
}


.subtitle2 {
  background: white;
  width: 100%;
  height: 6rpx;
  border-radius: 0 0 10rpx 10rpx;
}

/*人民币符号*/

.carts-price:before, .select-and-amount text:last-child:before {
  content: '';
}

.select-and-amount {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 26rpx;
  background: #fff;
}
.select-and-amount-mark {
  position: absolute;
  top: 10rpx;
  left: 20rpx;
  font-size: 28rpx;
}

.coupons-list {
  position: relative;
  top: 0;
  left: 0;
  display:flex;
  flex-direction:row;
}
.coupon-item-banner {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  align-content: center;
}
.counpons-item {
  background-size: 552rpx 246rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 552rpx;
  padding: 55rpx 0 20rpx 0;
}
.counpons {
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  align-content: center;
  background: #fff;
  margin: 10rpx auto;
  width: 100%;
}
.coupon-name {
  font-size: 32rpx;
  color: #ebdddd;
  line-height: 26rpx;
  width: 100%;
}

.coupon-content {
  font-size: 36rpx;
  color: #ebdddd;
  line-height: 2;
  width: 100%;
  
}

.coupon-dueday {
  font-size: 22rpx;
  color: #ebdddd;
  line-height: 1.5;
  width: 100%;
  
}
.coupon-footer {
  font-size: 22rpx;
  color: #ebdddd;
  line-height: 1.5;
  width: 100%;
  
}

.order-num {
  display: flex;
  flex-direction: row;
  justify-content: left;
  align-items: center;
  align-content: center;
  text-align: left;
  width: 100%;
  margin-bottom: 10rpx;
  border-top: 1rpx solid #F2F2F2;
  background: #fff;
  font-size: 28rpx;
  padding: 15rpx 30rpx;
}

.action-panel {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background: #fff;
  z-index: 9999;
  border-top: 1rpx solid #eee;
}


.confirmbtn {
  display:flex;
  flex-direction: row;
  justify-content: center;
  width: 100%;
  position: relative;
  bottom: 0;
  /*
  top:590rpx;
  */
}
.confirmbtn button {
  background-color: #FF952D !important;
  color: #fff !important;
  margin: 0 !important;
  border-radius: 0 !important;
  line-height: 3;
}
.confirmbtn button::after {
  border: 0;
  margin: 0;
}
.confirmbtn form {
  width: 100%;
  text-align: center;
}