page {
  background: #f2f2f2;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.startBar {
  height: 75rpx;
  background: #333;
  width: 100%;
  position: fixed;
  top: 0;
  padding: 5rpx;
  border-bottom: 0px solid #f2f2f2;
  z-index: 99;
}
.top-title {
  height: 75rpx;
  background: #333;
  width: 100%;
  position: fixed;
  top: 0;
  padding: 5rpx;
  border-bottom: 0px solid #f2f2f2;
  z-index: 99;
}
.top-title-item {
  display: flex;
  flex-direction: row;
  width: 100%;
  color: #fff;
  text-align: center;
  line-height: 75rpx;
  font-size: 48rpx;
  padding: 10rpx;
}
.top-title-item .title-image {
  width: 70rpx;
  height: 70rpx;
  margin-right: 1rpx;
  margin-top: 0rpx;
  margin-left: 20rpx;
}


.share-goods {
  width: 100%;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
  position: absolute;
  top: 0;
  z-index: 9; 
}

.share-buttons {
  display: flex;
  flex-direction: row;
  justify-content:space-around;
  align-items: center;
  margin-right:20rpx;
  padding: 0 auto;
}
.share-buttons .btnshare {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background:rgba(0, 0, 0, 0.5);
  font-size: 24rpx;
  color: #fff;
  padding: 6rpx 20rpx;
  line-height: 2;
  border-radius: 10rpx;
}
.share-buttons .shareto {
  width: 32rpx;
  height: 32rpx;
  margin:8rpx 6rpx 0 0;
}
.order-item-banner {
  width: 100%;
  display: flex;
  flex-direction: column;
  text-align: center;
  align-content: left;
  padding: 40rpx;
}

.order-item {
  width: 85%;
  display: flex;
  flex-direction: row;
  text-align: center;
  align-content: center;
  background: white;
  margin: 20rpx auto;
  border-radius: 20rpx;
}

/*user info*/

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: white;
  margin-top: 20rpx;
}

.userinfo-avatar {
  width: 80rpx;
  height: 80rpx;
  margin: 10rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #444;
  font-size: 32rpx;
  font-weight: bold;
}

/*每行单元格*/

.carts-item {
  display: flex;
  flex-direction: column;
  height: 260rpx;
  /*width属性解决标题文字太短而缩略图偏移*/
  width: 100%;
  padding: 10rpx;
}

.carts-image {
  margin: 20rpx;
  width: 200rpx;
  height: 160rpx;
}

/*每行单元格*/

.gifts-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  /*width属性解决标题文字太短而缩略图偏移*/
  width: 100%;
  padding: 10rpx 10rpx;
}

.gifts-image {
  margin: 20rpx;
  width: 150rpx;
  height: 150rpx;
}

.modalbox {
  background: #ffffff;
  margin-top: 10px;
}

.card-register-input {
  width: 100%;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.card-register-input input {
  width: 50%;
  height: 40rpx;
  line-height: 40rpx;
  margin: 0 0 20rpx 10rpx;
  padding: 15rpx;
  font-size: 24rpx;
  color: #999;
  background: #F3F3F3;
  border-radius: 14rpx;
}
.card-register-input textarea {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  background: #F3F3F3;
  width:420rpx;
  height:200rpx;
  padding: 10rpx;
  border-radius: 14rpx;
}


.card-name-input {
  width: 100%;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.card-name-input input {
  width: 50%;
  height: 40rpx;
  line-height: 40rpx;
  margin: 0 0 20rpx 10rpx;
  padding: 15rpx;
  font-size: 24rpx;
  color: #999;
  background: #F3F3F3;
  border-radius: 14rpx;
}
.card-name-input textarea {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  background: #F3F3F3;
  width:420rpx;
  height:200rpx;
  padding: 10rpx;
  border-radius: 14rpx;
}

/*立即按钮*/
.sentbtn {
  position: relative;
  bottom:0;
  margin-bottom: 100rpx;
  display: flex;
  flex-direction:column;
  justify-content: space-around;
  align-content: center;
  align-items: center;
  margin: 60rpx auto 20rpx auto;
  width: 100%;
  height: 190rpx;
}

.sentbtn button {
  line-height: 90rpx;
  width: 60%;
  height: 90rpx;
  /*background: linear-gradient(to bottom, #e34c55 0%,#e30715 100%);*/
  background: #e34c55;
  color: white;
  font-size: 36rpx;
  border-radius: 20rpx;
  -webkit-box-shadow: 0px 6rpx 0px rgba(219, 31, 5, 1);
  -moz-box-shadow: 0px 6rpx 0px rgba(219, 31, 5, 1);
  box-shadow: 0px 6rpx 0px rgb(179, 22, 2);
  display: table;
}

.sentbtn text {
  font-size: 24rpx;
  color: #999;
}

/**/

.order_note {
  width: 80%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin: 20rpx auto;
}

.order_note text {
  width: 100%;
  font-size: 30rpx;
  text-align: center;
  color: #444;
}

.order_note_end {
  margin-left: 10rpx;
  width: 100%;
  display: inline-block;
  font-size: 14px;
}

.order_note_end text {
  margin-left: 10rpx;
  font-size: 14px;
  background: white;
  color: blace;
}

.from-image {
  width: 10%;
  height: 50rpx;
  border-radius: 50%;
  margin-top: 40rpx;
}

/*logo部图片*/

.logo-image {
  width: 80rpx;
  height: 50rpx;
  border-radius: 50%;
}

/*右部描述*/

.carts-text {
  margin-left: 10rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

/*右上部分标题*/

.carts-title {
  font-size: 24rpx;
  color: #707070;
  line-height: 20rpx;
  width: 100%;
}

/*隐藏元素*/

.hidden {
  display: none;
}

/*顶部主题*/

.subject {
  width: 100%;
  height: 120rpx;
  line-height: 60rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  margin-top: 0rpx;
  padding-top:10rpx;
  background: #1d1d1d;
}

.subject text {
  width: 100%;
  font-size: 28rpx;
  color: #999;
  text-align: center;
}

.subject image {
  width: 45%;
  height: 100rpx;
  margin: 10rpx;
}

.subject .toptitle {
  font-size: 35rpx;
  font-weight: bold;
  color: #fff;
}

.swiper_box {
  width: 100%;
}

.swiper_box .slide-image {
  width: 100%;
  height: 80%;
  display: inline-block;
  overflow: hidden;
}

swiper-item image {
  width: 100%;
  display: inline-block;
  overflow: hidden;
}

