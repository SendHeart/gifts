@import "../../lib/style/font.wxss";
 
.Hui-iconfont {
 font-family: "Hui-iconfont";
 font-size:24rpx;
}
.iconv-uparrow:after {
  content:'\e6d6';
}
.iconv-downarrow:after {
  content:'\e6d5';
}
page {
  background-color:#1d1d1d;
  overflow-x: hidden;
  width: 100%;
  height: 100%;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.top-title {
  height: 75rpx;
  background: #333;
  width: 100%;
  position: absolute;
  top: 0;
  padding: 5rpx;
  border-bottom: 0px solid #f2f2f2;
  z-index: 99;
}
.top-title-item {
  display: flex;
  flex-direction: row;
  width: 100%;
  color: #fff;
  text-align: center;
  line-height: 75rpx;
  font-size: 48rpx;
  padding: 10rpx;
}
.top-title-item .title-image {
  width: 70rpx;
  height: 70rpx;
  margin-right: 1rpx;
  margin-top: 0rpx;
  margin-left: 20rpx;
}
.container {
    width: 100%;
    height: 100%;
    overflow-x: hidden;
    margin: 45rpx 0 0 0;
    padding-bottom: 100rpx;
}

.all-classify{ 
  width: 100%;
  background: rgba(1, 1, 1, 0.9); 
  display: flex;
  color: #fff;
  flex-direction: column;
  align-items: center;
  text-align: center;
  white-space: wrap;
  top:0;
  position: fixed;
  z-index: 999;
} 
.catg-title {
  display: flex;
  flex-direction: row;
  width: 100%;
  background: #1d1d1d;

}
.catg-titletext {
  font-size:32rpx;
  width:95%;
  align-items:right;
}
.catg-claps {
  width:5%;
  align-items:right;
}
.catg-claps image {
  width:20rpx;
  height:20rpx;
  padding:20rpx;
  margin-left:-20rpx;
}
.all-classify-list{ 
  display:flex;
  flex-direction: row;
  flex-wrap:wrap;
  color: #eeeeee;
  text-align: left;
   margin-bottom: 30rpx;
   padding: 10rpx ;
} 
.all-classify-item {
  color: #eeeeee;
  width: 150rpx;
  height: 50rpx;
  text-align: center;
  /* line-height: 72rpx; */
  font-size: 26rpx;
  border-radius: 10rpx;
  margin:10rpx 15rpx;
  line-height: 50rpx;
}
.all-classify-item-active{
  color: #fff;
  font-size: 26rpx;
  background: #E52445;
}

.top-bar-list{  
  display: flex;
  direction: row;
  align-items: left;
  text-align: left;
} 

.banner {
    width: 100%;
    height: 60px;
    padding-top: 0rpx;
    background: #1d1d1d; 
    margin-bottom:5px;
    z-index: 99;
}

/*导航 nav*/
.top-bar {
    white-space: nowrap;
    text-align: center;
    width: 90.5%;
    height: 30px;
    overflow-y: hidden;
    background: #1d1d1d; 
    z-index:99;
    margin-bottom: 10rpx;
}
.top-bar-item {
    display:inline-block;
    color: #fff;
    text-align: center;
    line-height: 30px;
    font-size: 28rpx;
    padding: 0 20rpx;
    margin: 0 10rpx;
    border-radius: 10rpx;
}
.top-bar-active {
  color: #fff;
  background: #E52445;
}
.top-bar-image {
  position: absolute; 
  right: 0; 
  width:20px;
  height:20px;
  z-index:999;
  margin-top:7px;
  padding-right: 15rpx;
}
/*辅助导航 nav2*/
.top-bar2 {
  display:flex;
  flex-direction: row;
  justify-content: center;
  height: 60rpx;
  background: #444; 
  width:100%;
  position: absolute;
  z-index:99;
}
.top-bar-item2 {
  display:flex;
  flex-direction: row;
  justify-content: center;
  width: 33.3%;
  color: #ccc;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
}
.top-bar-active2 {
  color: #fff;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
    z-index:99;
}

.venues_box {
  padding: 10rpx 0 100rpx 0;
  margin: 0 auto;
  width: 96%;
  overflow: hidden;
  z-index: 1;
}

.venues_item {
  margin-bottom:15rpx;
  background: #444;
  border-radius: 14rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
  display: inline-block;
}
.box-left,.box-right{
  display: inline-block;
  vertical-align: top;
  width: 49%;
}
.box-right{
  float: right;
}
.venues_item .mainpic {
  display: block;
  width: 354rpx;
  height: 354rpx;
}
.venues_item .img-discount {
  color: #fff;
  font-size: 22rpx;
  background: #EC4844;
  text-align: center;
  padding: 0 8rpx;
  border-radius: 6rpx;
  margin-right: 8rpx;
}
.venues_item .goods-org {
  color: #fff;
  font-size: 22rpx;
  background: #EC4844;
  text-align: center;
  padding: 0 8rpx;
  border-radius: 6rpx;
}
.venues_item .goods_name {
  color: #fff;
  font-size: 26rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  padding:10rpx auto;
  padding-left: 10rpx;
}

.venues_item .goods_tag {
  color: #fc9326;
  font-size: 20rpx;
  padding: 2rpx 5rpx;
  border: 2rpx solid #fc9326;
  border-radius: 6rpx;
}
.venues_item .goods-tags {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding:10rpx 0 0 10rpx;
}
.venues_item .goods_tag image{
  width: 10%;
  height: 30rpx;
}
.venues_item .goods_prom {
  display: block;
  color: #999;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: wrap;
  padding:0 10rpx;
}

.price-list {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}

.price-market {
  text-indent: 0;
  color: #999;
  font-size: 24rpx;
  line-height: 1.8;
  padding-right: 3rpx;
  text-decoration:line-through;
  display: none;
}
.price-market::first-letter {
  font-size: 20rpx;
}
.price-now {
  text-indent: 0;
  color: #eee;
  font-size: 26rpx;
}
.price-now::first-letter {
  font-size: 20rpx;
}
.price-now .sfont {
  font-size: 20rpx;
}

.left-tag {
  height: 28rpx;
  line-height: 28rpx;
  color: #fc9326;
  font-size: 20rpx;
  text-align: center;
  border-radius: 6rpx;
  padding: 6rpx 6rpx;
}

.goods_bottom {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx;
  text-indent: 0;
}
.search {
	width: 100%;
	height: 80rpx;
  display: flex;
	flex-direction: row;
	justify-content: center;/*子项横轴上居中对齐*/
	align-items: center;/*子项纵轴上居中对齐*/
  padding: 5rpx;
}

.search .wx-input {
	width: 70%;
	height: 30rpx;
	border: 1px solid #bfbfbf;
	border-radius: 120rpx;
  padding-left: 5rpx;
	/*margin: 30rpx 0 0 38rpx;*/
}

.search image {
	width: 5%;
	height: 40rpx;
	border: 0px solid #bfbfbf;
  margin-left:10rpx;
}
/* 返回顶部 */
.goTop{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;  
  height: 80rpx;
  width: 80rpx;
  position: fixed;
  bottom: 30rpx;
  background: rgba(0,0,0,.3);
  right: 30rpx;
  border-radius: 50%;
  color:#fff;
  font-size: 8;
  z-index: 999;
}

.scroll-view-x{
   background-color: #fff;
   white-space: nowrap;
}

.loading-show {
  width: 100%;
  /* 
  position: fixed; 
  bottom:0; 
  */
  height: 50rpx;
  line-height: 50rpx;
  display:none;
  flex-direction: row;
  justify-content: center;
  font-size: 30rpx;
  color: #f2f2f2;
  margin-bottom:50rpx;
  z-index: 1000;
}

